{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@clampy-js/clampy": "github:clampy-js/clampy",
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "aws-amplify": "^1.1.9",
    "clamp-js": "^0.7.0",
    "connected-react-router": "^5.0.1",
    "line-clamp": "0.0.9",
    "lint-staged": "^8.0.4",
    "mock-amplify-auth": "^2.0.0",
    "pretty-quick": "^1.8.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "react-text-truncate": "^0.14.0",
    "react-truncate-markup": "^3.0.1",
    "redux": "^4.0.1",
    "redux-api-middleware": "^3.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serverless-s3-sync": "^1.7.1",
    "string-to-color": "^2.0.1"
  },
  "scripts": {
    "lint": "eslint src",
    "format": "prettier --write \"**/*.js\"",
    "fetch-deployment-config": "node scripts/fetch-deployment-config",
    "husky": "^1.3.1",
    "start": "react-scripts start",
    "react-app-build": "node node_modules/react-scripts/bin/react-scripts build",
    "build": "npm run fetch-deployment-config && npm run react-app-build",
    "test": "npm run lint",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npm run lint",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "awscred": "^1.4.2",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react-app": "^4.0.1",
    "prettier": "^1.16.4"
  }
}
