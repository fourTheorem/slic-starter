{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "format": "eslint --cache --fix \"**/*.js\"",
    "fetch-deployment-config": "node scripts/fetch-deployment-config",
    "start": "SLIC_STAGE=local react-scripts start",
    "start-any": "react-scripts start",
    "react-app-build": "react-scripts build",
    "build": "npm run fetch-deployment-config && npm run react-app-build",
    "test": "npm run lint",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@aws-amplify/api": "^4.0.42",
    "@aws-amplify/auth": "^4.5.6",
    "@aws-amplify/core": "^4.5.6",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "awscred": "^1.5.0",
    "buffer": "^6.0.3",
    "clamp-js": "^0.7.0",
    "connected-react-router": "^6.9.2",
    "date-fns": "^2.28.0",
    "http-proxy-middleware": "^2.0.6",
    "line-clamp": "1.0.0",
    "mock-amplify-auth": "^2.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.3",
    "react-text-truncate": "^0.14.0",
    "react-truncate-markup": "^3.0.1",
    "redux": "^4.2.0",
    "redux-api-middleware": "^3.2.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "serverless-s3-sync": "^3.0.0",
    "string-to-color": "^2.2.2",
    "typescript": "^4.7.3",
    "yaml": "^2.1.1"
  }
}
